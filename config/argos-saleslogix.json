{
    "dojoPath":  "argos-sdk/libraries/dojo",
    "dojoCache": "dojo.cache",
    "dojoCompilation": ["dojo.js"],
    "projects": [{
        "name": "Argos SDK",
        "path": "argos-sdk/src",
        "alias": "argos-sdk",
        "deployAs": "content/javascript/argos-sdk.js",
        "outputPath": "argos-sdk/build/release.jsb2",
        "templatePath": "argos-sdk/build/release-template.jsb2"
    },{
        "name": "SalesLogix Mobile",
        "path": "products/argos-saleslogix/src",
        "alias": "argos-saleslogix",
        "deployAs": "content/javascript/argos-saleslogix.js",
        "outputPath": "products/argos-saleslogix/build/release.jsb2",
        "templatePath": "products/argos-saleslogix/build/release-template.jsb2",
        "includeDojo": {
            "name": "SalesLogix Mobile Dojo Dependencies",
            "deployAs": "content/dojo/dojo-dependencies.js"
        }
    }]
}